# for w32codecs, livdvdcss etc.
d-i apt-setup/local0/repository string http://debian-multimedia.org etch main 
d-i apt-setup/local0/comment string w32codecs and libdvdcss

# for oem-config (not for debian yet: only for ubuntu)
d-i apt-setup/local1/repository string http://lkcl.net/debian etch main
d-i apt-setup/local1/comment string oem config

# for msttcorefonts, flashplayer etc.
d-i apt-setup/non-free boolean true
d-i apt-setup/contrib boolean true

